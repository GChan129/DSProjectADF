{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFDSProject2"
		},
		"BlobStorageCSV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageCSV'"
		},
		"DSProjectDB_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DSProjectDB_ls'"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"SqlServer_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_ls'"
		},
		"RestServiceFX_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestServiceFX'"
		},
		"HttpServerFX_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'HttpServerFX'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dsproject.dfs.core.windows.net/"
		},
		"RestServiceFX_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.apilayer.com/currency_data"
		},
		"RestServiceFX_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "graciechan@gmail.com"
		},
		"HttpServerFX_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.apilayer.com/currency_data"
		},
		"HttpServerFX_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "graciechan@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFDSProject2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobStorageCSV_connectionString": {
						"value": "[parameters('BlobStorageCSV_connectionString')]"
					},
					"DSProjectDB_ls_connectionString": {
						"value": "[parameters('DSProjectDB_ls_connectionString')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"SqlServer_ls_connectionString": {
						"value": "[parameters('SqlServer_ls_connectionString')]"
					},
					"RestServiceFX_password": {
						"value": "[parameters('RestServiceFX_password')]"
					},
					"HttpServerFX_password": {
						"value": "[parameters('HttpServerFX_password')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"RestServiceFX_properties_typeProperties_url": {
						"value": "[parameters('RestServiceFX_properties_typeProperties_url')]"
					},
					"RestServiceFX_properties_typeProperties_userName": {
						"value": "[parameters('RestServiceFX_properties_typeProperties_userName')]"
					},
					"HttpServerFX_properties_typeProperties_url": {
						"value": "[parameters('HttpServerFX_properties_typeProperties_url')]"
					},
					"HttpServerFX_properties_typeProperties_userName": {
						"value": "[parameters('HttpServerFX_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFDSProject2_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFDSProject2_ArmTemplate_0')]"
			]
		}
	]
}