{
	"name": "CSVcommaDL_ForExcel",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@substring(dataset().FileName,0,sub(length(dataset().FileName),4))",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(substring(utcnow(),0 ,4 ),\n'/',\nsubstring(utcnow(),5 ,2 ),\n'/',\nsubstring(utcnow(),8 ,2 )\n)",
					"type": "Expression"
				},
				"fileSystem": "datalake"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "'"
		},
		"schema": []
	}
}